
<table>
  <thead>
    <tr>
      <td><h3>管理者　ログイン</h3></td>
    </tr>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <tr>
  <div class="field">
    <td><p>メールアドレス</p></td>
    <td><%= f.email_field :email, autofocus: true, autocomplete: "email", :size=>"50" %></td>
  </div>
  </tr>
  
  <tr>
  <div class="field">
    <td><p>パスワード</p></td>
    <td><%= f.password_field :password, autocomplete: "current-password", :size=>"50" %></td>
  </div>
  </tr>
  </thead>
  
  <tbody>
    <tr>
      <tr>
         
      </tr>
      <td>
      <% if devise_mapping.rememberable? %>
       <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
      </div>
      <% end %>  
      </td>
    </tr>
    <tr>
      <td>
        <div class="actions">
          <%= f.submit "ログイン" %>
         </div>
      </td>
    </tr>

  </tbody>
  </table>
   
<% end %>

<%= render "admin/shared/links" %>
